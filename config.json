{
    "DEFAULT": {
    "upscale_factor": 4,
    "crop_size": 112,
    "PATH_TO_TRAIN_LR_DATA": "~/data/wind/low-res/bicubic",
    "PATH_TO_TRAIN_HR_DATA": "~/data/wind/hi-res/",
    "PATH_TO_VALID_LR_DATA": "~/data/wind/val/low-res/bicubic",
    "PATH_TO_VALID_HR_DATA": "~/data/wind/val/hi-res"
  },

  "TRAIN_PSNR": {
    "number_epochs": 1,
    "batch_size": 5,
    "learning_rate": 1e-4,
    "val_freq": 5e3,
    "save_checkpoint_freq": 1e4,
    "loss_pix_weight": 1e-2,
    "loss_feature_weight": 1.0,
    "log_path": "../output/Statistics/logs_PSNR",
    "path_to_training_results": "../output/training_results_PSNR",
    "path_to_save_model": "../output/PSNR_model"
  },

  "TRAIN_ESRGAN": {
    "scale": "x4",
    "warm_start": "True",
    "n_epoch_start": 23,
    "n_epoch_end": 26,
    "batch_size": 3,
    "learning_rate_G": 1e-4,
    "learning_rate_D": 1e-4,
    "D_update_ratio": 1,
    "D_init_iters": 0,
    "val_freq": 1e4,
    "save_checkpoint_freq": 1e4,
    "loss_pix_weight": 5e-3,
    "loss_feature_weight": 1.0,
    "loss_gan_weight": 0.005,
    "log_path": "../output/Statistics/logs_x4"
  },

  "EVALUATE": {
    "SERVICE": "travis-ci",
    "HOOK_URL": "web-hooking-url-from-ci-service"
  },

    "PREDICT": {
    "path_to_model": "../output/models/netG_x4_epoch=25",
    "path_to_image": "~/data/wind/val/low-res/bicubic/0-d01_T2_2001.png",
    "path_to_save_image": "../images/0-d01_T2_2001.png"
  }
}
